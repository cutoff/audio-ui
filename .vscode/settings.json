{
    "vitest.include": [
        "packages/core/vitest.config.{ts,js,mjs,cjs}",
        "packages/react/vitest.config.{ts,js,mjs,cjs}",
        "packages/react/vite.config.{ts,js,mjs,cjs}"
    ],
    "vitest.exclude": [
        "**/node_modules/**",
        "**/dist/**",
        "**/links/**",
        "**/*.timestamp-*.mjs",
        "**/vite.config.ts.timestamp-*",
        "**/vitest.config.ts.timestamp-*"
    ],
    "vitest.commandLine": "pnpm exec vitest",
    "vitest.disableWatchOnStartup": true,
    "files.exclude": {
        "**/links/**": true,
        "**/*.timestamp-*.mjs": true,
        "**/vite.config.ts.timestamp-*": true,
        "**/vitest.config.ts.timestamp-*": true
    },
    "files.watcherExclude": {
        "**/links/**": true,
        "**/node_modules/**": true,
        "**/.git/**": true
    },
    "search.exclude": {
        "**/*.timestamp-*.mjs": true,
        "**/vite.config.ts.timestamp-*": true,
        "**/vitest.config.ts.timestamp-*": true,
        "**/links/**": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
